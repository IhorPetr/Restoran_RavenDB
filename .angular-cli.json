{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "restoran-raven-db"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "Fonts",
        "images"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "icomoon.css",
        "simple-line-icons.css",
        "../node_modules/animate.css/animate.css",
        "../node_modules/bootstrap-datetimepicker-npm/build/css/bootstrap-datetimepicker.css",
        "../node_modules/flexslider/flexslider.css",
        "../node_modules/bootstrap/dist/css/bootstrap.css"
      ],
      "scripts": [
        "../node_modules/modernizr/src/Modernizr.js",
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/jquery-easing/jquery.easing.1.3.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/bootstrap-datetimepicker-npm/build/js/bootstrap-datetimepicker.min.js",
        "../node_modules/moment/moment.js",
        "../node_modules/jquery.stellar/jquery.stellar.js",
        "../node_modules/flexslider/jquery.flexslider-min.js",
        "../node_modules/jquery-waypoints/waypoints.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
